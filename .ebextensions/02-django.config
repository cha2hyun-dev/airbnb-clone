container_commands:
 01_migrate:
  command: "django-admin migrate"
  leader_only: true
 02_compilemessages:
  command: "django-admin compilemessages"
 03_createsu:
  command: "django-admin createsu"
 04_collectstatic:
  command: "django-admin collectstatic --noinput"
 05_django-admin seed_amenities
  command: "django-admin seed_amenities"
 06_django-admin seed_facilities
  command: "django-admin seed_facilities"
 07_django-admin seed_roomtypes
  command: "django-admin seed_roomtypes"
 08_django-admin seed_users
  command: "django-admin seed_users --number 50"
 09_django-admin seed_rooms
  command: "django-admin seed_rooms --number 150"
 10_django-admin seed_reviews
  command: "django-admin seed_reviews --number 50"
 11_django-admin seed_lists
  command: "django-admin seed_lists --number 50"
 12_django-admin seed_reservations
  command: "django-admin seed_reservations --number 50"
option_settings:
 aws:elasticbeanstalk:container:python:
  WSGIPath: config/wsgi.py
 aws:elasticbeanstalk:application:environment:
  DJANGO_SETTINGS_MODULE: config.settings
